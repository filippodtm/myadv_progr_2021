#include <iostream>
#include <string>


int main(){
  double n;
  std::string grandezza;
  std::string properunit;

  std::cout << "measured quantity: ";
  std::cin >> grandezza;
  if(grandezza == "time")
    {
    std::cout << "number with proper unit of measure: ";
    std::cin >> n;
    std::cout << " ";
    std::cin >> properunit;
    
    if(properunit=="h")
      std::cout << "\n" << n*3600 << " s \n";
    else if(properunit=="min")
      std::cout << "\n" << n*60 << " s \n";
    }
  else
      std::cout<<"BRUH \n";
  return 0;
}












